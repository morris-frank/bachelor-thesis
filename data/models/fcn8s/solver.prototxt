train_net: "data/models/fcn8s/train.prototxt"
#test_net: "data/models/fcn8s/val.prototxt"
#test_iter: 736
# make test net, but don't invoke it from the solver itself
test_interval: 999999999
display: 20
average_loss: 20
lr_policy: "fixed"
# lr for unnormalized softmax
base_lr: 1e-2
# high momentum
momentum: 0.99
# no gradient accumulation
iter_size: 1
max_iter: 100000
weight_decay: 0.0005
snapshot: 1000
snapshot_prefix: "data/models/fcn8s/snapshot/train"
test_initialization: false
